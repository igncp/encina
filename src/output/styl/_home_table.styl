#home-table
  margin-top 40px
  box-shadow 0px 0px 20px 1px #ddd
  tr
    &:nth-child(odd)
      background-color #F2FFF4
      
  td
    padding 0
    text-align center
    vertical-align middle
    
    &.project-name
      font-size 20px
      
  ul
    list-style-type none
    margin 0
    padding 0
    width 100%
  li
    height 55px
    padding-top 10px
    border-left 1px solid #ddd
    &.not-last
      border-bottom 1px solid #ccc
